LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US
#pragma code_page(1252)

IDR_MAIN_MENU MENUEX PRELOAD DISCARDABLE 
BEGIN
	POPUP N_("&File"), IDM_FILE_MENU
	BEGIN
		MENUITEM N_("&Open Hypertext...\tCtrl+O"),	IDM_FILE_OPEN
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&Save text...\tCtrl+S"),	        IDM_FILE_SAVE
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&Catalog\tAlt+K"),		        IDM_FILE_CATALOG
		MENUITEM N_("&Default file\tAlt+D"),	        IDM_FILE_DEFAULT
		MENUITEM N_("&Remarker\tAlt+R"),		IDM_FILE_REMARKER
		MENUITEM "", -1, MFT_SEPARATOR
		POPUP N_("Open Recent"), IDM_FILE_RECENTMENU
		BEGIN
			MENUITEM "recent-1\tCtrl+1",	IDM_FILE_RECENT_1
			MENUITEM "recent-2\tCtrl+2",	IDM_FILE_RECENT_2
			MENUITEM "recent-3\tCtrl+3",	IDM_FILE_RECENT_3
			MENUITEM "recent-4\tCtrl+4",	IDM_FILE_RECENT_4
			MENUITEM "recent-5\tCtrl+5",	IDM_FILE_RECENT_5
			MENUITEM "recent-6\tCtrl+6",	IDM_FILE_RECENT_6
			MENUITEM "recent-7\tCtrl+7",	IDM_FILE_RECENT_7
			MENUITEM "recent-8\tCtrl+8",	IDM_FILE_RECENT_8
			MENUITEM "recent-9\tCtrl+9",	IDM_FILE_RECENT_9
			MENUITEM "recent-10\tCtrl+0",	IDM_FILE_RECENT_10
		END
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&File info...\tCtrl+I"),	IDM_FILE_INFO
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&Close\tCtrl+U"),		IDM_FILE_CLOSE
		MENUITEM N_("&Quit\tCtrl+Q"),		IDM_FILE_QUIT
	END
	POPUP N_("&Edit"), IDM_EDIT_MENU
	BEGIN
		MENUITEM N_("Select &All\tCtrl+A"),         IDM_EDIT_SELECTALL
		MENUITEM N_("&Copy\tCtrl+C"),               IDM_EDIT_COPY
		MENUITEM N_("&Paste\tCtrl+V"),              IDM_EDIT_PASTE
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&Find\tCtrl+F"),               IDM_EDIT_FIND
		MENUITEM N_("Find &Next\tCtrl+G"),          IDM_EDIT_FINDNEXT
	END
	POPUP N_("&Navigate"), IDM_NAV_MENU
	BEGIN
		MENUITEM N_("Previous logical page\tNumpad -"),       IDM_NAV_PREV
		MENUITEM N_("Next logical page\tNumpad +"),           IDM_NAV_NEXT
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("Previous physical page\tNumpad /"),      IDM_NAV_PREVPHYS
		MENUITEM N_("Next physical page\tNumpad *"),          IDM_NAV_NEXTPHYS
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("First page"),                            IDM_NAV_FIRST
		MENUITEM N_("Last page"),                             IDM_NAV_LAST
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("Contents\tAlt+T"),                       IDM_NAV_TOC
		MENUITEM N_("Index\tAlt+X"),                          IDM_NAV_INDEX
		MENUITEM N_("Show help page\tAlt+H"),                 IDM_NAV_HELP
		MENUITEM "", -1, MFT_SEPARATOR
		POPUP N_("&Bookmarks"), IDM_NAV_BOOKMARKSMENU
		BEGIN
			MENUITEM "bookmark-1\tF1",      IDM_NAV_BOOKMARK_1
			MENUITEM "bookmark-2\tF2",      IDM_NAV_BOOKMARK_2
			MENUITEM "bookmark-3\tF3",      IDM_NAV_BOOKMARK_3
			MENUITEM "bookmark-4\tF4",      IDM_NAV_BOOKMARK_4
			MENUITEM "bookmark-5\tF5",      IDM_NAV_BOOKMARK_5
			MENUITEM "bookmark-6\tF6",      IDM_NAV_BOOKMARK_6
			MENUITEM "bookmark-7\tF7",      IDM_NAV_BOOKMARK_7
			MENUITEM "bookmark-8\tF8",      IDM_NAV_BOOKMARK_8
			MENUITEM "bookmark-9\tF9",      IDM_NAV_BOOKMARK_9
			MENUITEM "bookmark-10\tF10",    IDM_NAV_BOOKMARK_10
			MENUITEM "bookmark-11\tF11",    IDM_NAV_BOOKMARK_11
			MENUITEM "bookmark-12\tF12",    IDM_NAV_BOOKMARK_12
		END
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("Back one page\tBackSpace"),    IDM_NAV_BACK
		MENUITEM N_("Clear stack\tAlt+E"),          IDM_NAV_CLEARSTACK
	END
	POPUP N_("&Options"), IDM_OPT_MENU
	BEGIN
		MENUITEM N_("&Font...\tAlt+Z"),             IDM_OPT_SELECTFONT
		MENUITEM N_("&Colors..."),                  IDM_OPT_SELECTCOLORS
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&Output..."),                  IDM_OPT_OUTPUT
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&Alternative font\tCtrl+Z"),   IDM_OPT_ALTFONT
		MENUITEM N_("&Expand multiple spaces\tCtrl+L"),  IDM_OPT_EXPANDSPACES
		// MENUITEM N_("&Scale bitmaps\tCtrl+B"),   IDM_OPT_SCALEBITMAPS
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&Settings..."),                IDM_OPT_PREFERENCES
	END
	POPUP N_("&Help"), IDM_HELP_MENU
	BEGIN
		MENUITEM N_("&Contents"),                   IDM_HELP_CONTENTS
		MENUITEM N_("&Index"),                      IDM_HELP_INDEX
		MENUITEM "", -1, MFT_SEPARATOR
		MENUITEM N_("&About"),                      IDM_HELP_ABOUT
	END
END


