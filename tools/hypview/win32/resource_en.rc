        DEFPUSHBUTTON   N_("as page"), IDC_HYPFIND_PAGES, 80,50,50,14, WS_GROUP
        PUSHBUTTON      N_("as reference"), IDC_HYPFIND_REF, 150,50,50,14, WS_GROUP
        PUSHBUTTON      N_("in all pages"), IDC_HYPFIND_ALL_PAGE, 20,75,50,14, WS_GROUP
        PUSHBUTTON      N_("...of all Hypertexts"), IDC_HYPFIND_ALL_HYP, 80,75,60,14, WS_GROUP
        PUSHBUTTON      N_("&Cancel"), IDCANCEL, 150,75,50,14, WS_GROUP
END
